{
  "name": "@cocalc/sync-fs",
  "version": "0.1.0",
  "description": "CoCalc Filesystem Sync",
  "main": "./dist/lib/index.js",
  "exports": {
    ".": "./dist/lib/index.js",
    "./*": "./dist/*.js",
    "./lib/*": "./dist/lib/*.js"
  },
  "files": [
    "dist/**",
    "bin/**",
    "README.md",
    "package.json"
  ],
  "scripts": {
