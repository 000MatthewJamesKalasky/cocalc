{
  "name": "@cocalc/share",
  "version": "0.6.0",
  "description": "Landing pages for the free version of CoCalc.",
  "exports": {
    "./init": "./lib/init.js",
    "./lib/*": "./dist/lib/*.js"
  },
  "files": [
    "dist/*",
    ".next/*",
    "styles/*",
    "public/*",
    "pages/*",
    "lib/*",
    "components/*",
    "index.js",
    "README.md",
    "package.json",
    "tsconfig.json",
    "next-env.d.ts",
    "next.config.js"
  ],
  "scripts": {
    "cocalc-dev": "BASE_PATH=/$COCALC_PROJECT_ID/port/3000 next dev",
    "cocalc-prod": "export BASE_PATH=/$COCALC_PROJECT_ID/port/3000 && next build && next start",
    "tsc": "npx tsc --build tsconfig-dist.json -w --pretty ",
    "dev": "next dev",
    "build": "rm -rf .next/cache/webpack/*development* && next build && npm run build-dist",
    "build-dist": "npx tsc --build tsconfig-dist.json",
    "start": "next start",
    "lint": "next lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sagemathinc/cocalc.git"
  },
  "author": "SageMath, Inc.",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/sagemathinc/cocalc/issues"
  },
  "homepage": "https://github.com/sagemathinc/cocalc#readme",
  "private": false,
  "workspaces": [
    "../frontend",
    "../util-node",
    "../util",
    "../cdn"
  ],
  "dependencies-comment": "IMPORTANT: we install everything (including typescript) as normal deps since it may be needed at runtime due to server settings!!",
  "dependencies": {
    "@cocalc/cdn": "^1.3.0",
    "@cocalc/frontend": "^1.1.0",
    "@cocalc/util": "^1.2.0",
    "@cocalc/util-node": "^1.1.0",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.14",
    "antd": "^4.16.8",
    "basic-auth": "^2.0.1",
    "express": "^4.17.1",
    "lru-cache": "^6.0.0",
    "next": "^11.1.0",
    "password-hash": "^1.2.2",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serve-index": "^1.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "eslint": "^7.31.0",
    "eslint-config-next": "11.0.1"
  }
}
