set -v

gcutil --service_version="v1" --project="sagemathcloud" adddisk  --zone="us-central1-a" --source_snapshot=compute-base-disk-2014-03-15-1302  --wait_until_complete compute4dc2

gcutil --service_version="v1" --project="sagemathcloud" addinstance --auto_delete_boot_disk --automatic_restart --disk=compute4dc2,mode=rw,boot --disk=compute4dc2-projects,mode=rw --disk=compute4dc2-projects2,mode=rw --disk=compute4-storage,mode=rw  --wait_until_running  --machine_type=n1-highmem-4 --zone="us-central1-a"  compute4dc2
