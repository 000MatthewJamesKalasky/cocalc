set -v

gcutil --service_version="v1" --project="sagemathcloud" adddisk  --zone="us-central1-a" --source_snapshot=`cat BASE`  --wait_until_complete compute1dc2

gcutil --service_version="v1" --project="sagemathcloud" addinstance --auto_delete_boot_disk --automatic_restart --disk=compute1dc2,mode=rw,boot --disk=compute1dc2-projects,mode=rw --disk=compute1dc2-projects2,mode=rw --disk=compute1-storage,mode=rw  --wait_until_running  --machine_type=n1-highmem-8 --zone="us-central1-a"  compute1dc2
